{
  "manifest_version": 2,
  "name": "google-no-tracking-url",
  "version": "2.2.0",
  "author": "Matias Agustin Mendez",
  "description": "It simply removes tracking code/redirect from Google search results.",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["background/http-interceptor.js"]
  },
  "applications": {
    "gecko": {
      "id": "jid1-zUrvDCat3xoDSQ@jetpack"
    }
  },
  "icons": {
    "96": "icon.png"
  }
}
